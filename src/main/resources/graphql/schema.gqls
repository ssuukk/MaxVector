type Query {
    bookById(id: ID): Book
    """embeddingById(id: ID): Embedding"""
    embeddingByClosest(vec: Float): [Embedding]
}

type Book {
    id: ID
    name: String
    pageCount: Int
    author: Author
}

type Author {
    id: ID
    firstName: String
    lastName: String
}

type Embedding {
    id: ID
    embedding: [Float]
}